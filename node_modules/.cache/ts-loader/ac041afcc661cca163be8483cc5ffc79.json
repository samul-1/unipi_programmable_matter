  },\n    position (): { x: number; y: number } {\n      if (this.point) {\n        return { x: this.point.x, y: this.point.y }\n      }\n\n      return { x: 0, y: 0 }\n    },\n    x () {\n      return this.position.x\n    },\n    y () {\n      return this.position.y\n    },\n    neighborPoints (): GridPoint[] {\n      return this.$store.getters.getNeighbors(this.point)\n    },\n    ...mapState(['move'])\n  }\n})\n"],"sourceRoot":""}]}